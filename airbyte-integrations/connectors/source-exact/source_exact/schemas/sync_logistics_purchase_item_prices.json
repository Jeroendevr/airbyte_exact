{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "sync/Logistics/PurchaseItemPrices",
  "properties": {
    "Timestamp": {
      "description": "Timestamp",
      "type": "integer"
    },
    "Account": {
      "description": "ID of the supplier",
      "type": "string"
    },
    "AccountName": {
      "description": "Name of the supplier account",
      "type": "string"
    },
    "Created": {
      "description": "Creation date",
      "type": "string",
      "format": "date-time"
    },
    "Creator": {
      "description": "User ID of creator",
      "type": "string"
    },
    "CreatorFullName": {
      "description": "Name of creator",
      "type": "string"
    },
    "Currency": {
      "description": "The currency of the price",
      "type": "string"
    },
    "DefaultItemUnit": {
      "description": "The default unit of the item",
      "type": "string"
    },
    "DefaultItemUnitDescription": {
      "description": "The description of the default item unit",
      "type": "string"
    },
    "Division": {
      "description": "Division code",
      "type": "integer"
    },
    "EndDate": {
      "description": "Together with StartDate this determines whether the price is active",
      "type": "string",
      "format": "date-time"
    },
    "ID": {
      "description": "Primary key",
      "type": "string"
    },
    "Item": {
      "description": "Item ID",
      "type": "string"
    },
    "ItemCode": {
      "description": "Code of Item",
      "type": "string"
    },
    "ItemDescription": {
      "description": "Description of Item",
      "type": "string"
    },
    "Modified": {
      "description": "Last modified date",
      "type": "string",
      "format": "date-time"
    },
    "Modifier": {
      "description": "User ID of modifier",
      "type": "string"
    },
    "ModifierFullName": {
      "description": "Name of modifier",
      "type": "string"
    },
    "NumberOfItemsPerUnit": {
      "description": "This is the multiplication factor when going from default item unit to the unit of this price.For example if the default item unit is 'gram' and the price unit is 'kilogram' then the value of this property is 1000.",
      "type": "number"
    },
    "Price": {
      "description": "The actual price of this purchase item",
      "type": "number"
    },
    "Quantity": {
      "description": "Minimum quantity to which the price is applicable",
      "type": "number"
    },
    "StartDate": {
      "description": "Together with EndDate this determines whether the price is active",
      "type": "string",
      "format": "date-time"
    },
    "Unit": {
      "description": "The unit code of the price",
      "type": "string"
    },
    "UnitDescription": {
      "description": "Description of the price unit",
      "type": "string"
    }
  }
}