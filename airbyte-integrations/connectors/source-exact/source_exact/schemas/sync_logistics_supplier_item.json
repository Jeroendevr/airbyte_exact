{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "sync/Logistics/SupplierItem",
  "properties": {
    "Timestamp": {
      "description": "Timestamp",
      "type": "integer"
    },
    "CopyRemarks": {
      "description": "Copy purchase remarks to purchase lines",
      "type": "integer"
    },
    "CountryOfOrigin": {
      "description": "Country of origin code",
      "type": "string"
    },
    "CountryOfOriginDescription": {
      "description": "Description of country of origin",
      "type": "string"
    },
    "Created": {
      "description": "Creation date",
      "type": "string",
      "format": "date-time"
    },
    "Creator": {
      "description": "User ID of creator",
      "type": "string"
    },
    "CreatorFullName": {
      "description": "Name of creator",
      "type": "string"
    },
    "Currency": {
      "description": "Currency of item price",
      "type": "string"
    },
    "CurrencyDescription": {
      "description": "Description of currency of item price",
      "type": "string"
    },
    "Division": {
      "description": "Division code",
      "type": "integer"
    },
    "DropShipment": {
      "description": "Indicates that the supplier will deliver the item directly to customer. Values: 0 = No, 1 = Yes, 2 = Optional",
      "type": "integer"
    },
    "EndDate": {
      "description": "Together with StartDate this determines whether the price is active",
      "type": "string",
      "format": "date-time"
    },
    "ID": {
      "description": "Primary key",
      "type": "string"
    },
    "Item": {
      "description": "Item ID",
      "type": "string"
    },
    "ItemCode": {
      "description": "Item code",
      "type": "string"
    },
    "ItemDescription": {
      "description": "Description of Item",
      "type": "string"
    },
    "ItemUnit": {
      "description": "Item Unit",
      "type": "string"
    },
    "ItemUnitCode": {
      "description": "Item Unit Code",
      "type": "string"
    },
    "ItemUnitDescription": {
      "description": "Item Unit Description",
      "type": "string"
    },
    "MainSupplier": {
      "description": "Indicates this is a main supplier",
      "type": "boolean"
    },
    "MinimumQuantity": {
      "description": "Minimum quantity of the item for purchase, only available for Wholesale & Distribution (Professional and Premium only)",
      "type": "number"
    },
    "Modified": {
      "description": "Last modified date",
      "type": "string",
      "format": "date-time"
    },
    "Modifier": {
      "description": "User ID of modifier",
      "type": "string"
    },
    "ModifierFullName": {
      "description": "Name of modifier",
      "type": "string"
    },
    "Notes": {
      "description": "Notes",
      "type": "string"
    },
    "PurchaseLeadTime": {
      "description": "The number of days between placing an order with a supplier and receiving items from the supplier",
      "type": "integer"
    },
    "PurchaseLotSize": {
      "description": "Lot size of the item for purchase, only available for Wholesale & Distribution (Premium only)",
      "type": "integer"
    },
    "PurchasePrice": {
      "description": "Purchase price. If neither active nor future price exists, it shows 0 when GET",
      "type": "number"
    },
    "PurchaseUnit": {
      "description": "Unit code",
      "type": "string"
    },
    "PurchaseUnitDescription": {
      "description": "Description of unit",
      "type": "string"
    },
    "PurchaseUnitFactor": {
      "description": "This is the multiplication factor when going from default item unit to the unit of this price",
      "type": "number"
    },
    "PurchaseVATCode": {
      "description": "VAT code",
      "type": "string"
    },
    "PurchaseVATCodeDescription": {
      "description": "Description of VAT",
      "type": "string"
    },
    "StartDate": {
      "description": "Together with EndDate this determines whether the price is active",
      "type": "string",
      "format": "date-time"
    },
    "Supplier": {
      "description": "Supplier ID",
      "type": "string"
    },
    "SupplierCode": {
      "description": "Supplier code",
      "type": "string"
    },
    "SupplierDescription": {
      "description": "Description of supplier",
      "type": "string"
    },
    "SupplierItemCode": {
      "description": "Supplier's item code",
      "type": "string"
    }
  }
}